def two_sum(a, k):
    
    
    if a is None:
        return None
        
    count = 0
        
    for i in range(len(a)):
        for j in range(i, len(a)):
            if a[i] + a[j] == k:
                count =+ 1
                
    return count
                
            
    
    
a = [2,3,4,5,3,2,6,8]
k = 10

two_sum(a, k)